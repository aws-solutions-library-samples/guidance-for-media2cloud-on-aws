{
  "name": "m2c-custom-resources",
  "version": "2.0.0",
  "description": "media2cloud cloudformation custom resource",
  "main": "index.js",
  "private": true,
  "scripts": {
    "pretest": "npm install",
    "test": "mocha *.spec.js",
    "build:clean": "rm -rf dist && mkdir -p dist",
    "build:copy": "cp -rv index*.js package.json lib dist/",
    "build:install": "cd dist && npm install --omit=dev",
    "build": "npm-run-all -s build:clean build:copy build:install",
    "zip": "cd dist && zip -rq"
  },
  "author": "aws-mediaent-solutions",
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.743.0",
    "@aws-sdk/client-cloudfront": "^3.743.0",
    "@aws-sdk/client-codebuild": "^3.743.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.743.0",
    "@aws-sdk/client-ecr": "^3.743.0",
    "@aws-sdk/client-iot": "^3.743.0",
    "@aws-sdk/client-mediaconvert": "^3.743.0",
    "@aws-sdk/client-neptune": "^3.743.0",
    "@aws-sdk/client-s3": "^3.743.0",
    "@aws-sdk/client-sns": "^3.743.0",
    "@aws-sdk/credential-providers": "^3.743.0",
    "@aws-sdk/s3-request-presigner": "^3.743.0",
    "@opensearch-project/opensearch": "^2.13.0",
    "@smithy/service-error-classification": "^2.1.5",
    "@smithy/smithy-client": "^2.5.1",
    "@smithy/util-retry": "^2.2.0",
    "adm-zip": "^0.4.16",
    "aws-xray-sdk-core": "^3.10.3",
    "mime": "^3.0.0"
  },
  "devDependencies": {
    "core-lib": "file:../layers/core-lib"
  }
}
