{
  "name": "m2c-custom-resources",
  "version": "1.0.0",
  "description": "media2cloud cloudformation custom resource",
  "main": "index.js",
  "private": true,
  "scripts": {
    "pretest": "npm install",
    "test": "mocha *.spec.js",
    "build:clean": "rm -rf dist && mkdir -p dist",
    "build:copy": "cp -rv index*.js package.json lib dist/",
    "build:install": "cd dist && npm install --omit=dev --omit=optional",
    "build": "npm-run-all -s build:clean build:copy build:install",
    "zip": "cd dist && zip -rq"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  },
  "dependencies": {
    "@opensearch-project/opensearch": "^2.10.0",
    "adm-zip": "^0.4.16",
    "aws-sdk": "^2.1642.0",
    "aws-xray-sdk": "^3.9.0"
  },
  "devDependencies": {
    "core-lib": "file:../layers/core-lib"
  },
  "license": "Apache-2.0"
}
